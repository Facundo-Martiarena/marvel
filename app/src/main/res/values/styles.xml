<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="search_view" parent="Widget.AppCompat.SearchView">
        <item name="android:background">@drawable/search_view_background</item>
        <item name="android:searchHintIcon">@drawable/custom_search_hint_icon</item>
        <item name="android:editTextColor">@color/black</item>
    </style>

    <style name="styleInputText" parent="Widget.Design.TextInputLayout">
        <item name="boxStrokeColor">@color/white</item>

    </style>

    <style name="styleSearchBar" parent="Widget.Material3.SearchView">
        <item name="editTextColor">#F3E5AB</item>
        <item name="android:textColorHint">#175381</item>

    </style>

    <style name="buttonActionStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:backgroundTint">#FFFFFF</item>


    </style>

    <style name="AlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>
</resources>